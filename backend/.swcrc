{
  "$schema": "https://json.schemastore.org/swcrc",
  "sourceMaps": true,
  "exclude": [
    "node_modules",
    "src/agent/build",
    "dist",
    "\\.d\\.ts$"
  ],
  "jsc": {
    "parser": {
      "syntax": "typescript",
      "decorators": true,
      "dynamicImport": true
    },
    "transform": {
      "legacyDecorator": true,
      "decoratorMetadata": true
    },
    "baseUrl": "./",
    "target": "es2020"
  },
  "module": {
    "type": "commonjs"
  }
}
